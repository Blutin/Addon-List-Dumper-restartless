{
  "name": "extension-list-dumper",
  "title": "Extension List Dumper (restartless)",
  "id": "extensionListDumper",
  "description": "",
  "author": "monkey413",
  "license": "GPL 3.0",
  "version": "0.1",


  "preferences": [{
        "name": "version",
        "title": "version",
        "description": "show the version of addons",
        "type": "bool",
        "value": true
    },
    {
  	"name": "description",
        "title": "description",
        "description": "show the description of addons",
        "type": "bool",
        "value": false
    },
    {
  	"name": "url",
        "title": "URL",
        "description": "show the homepage url of addons",
        "type": "bool",
        "value": false
    },
    {
  	"name": "id",
        "title": "ID",
        "description": "show the ID of addons",
        "type": "bool",
        "value": false
    },
    {
  	"name": "softInfo",
        "title": "Software Infomation",
        "description": "show firefox version and operating system",
        "type": "bool",
        "value": true
    },
    {
  	"name": "total",
        "title": "Total",
        "description": "show the total of items",
        "type": "bool",
        "value": false
    },
    {
  	"name": "date",
        "title": "Date",
        "description": "show the date of today",
        "type": "bool",
        "value": false
    },
    {
  	"name": "installDate",
        "title": "Install Date",
        "description": "show the install date of addons",
        "type": "bool",
        "value": false
    },
    {
  	"name": "active",
        "title": "Active",
        "type": "menulist",
	"value": "All",
        "options": [
      		{"value": "All", "label": "All"},
      		{"value": "Enabled", "label": "Enabled"},
		{"value": "Incompatible", "label": "Incompatible"},
		{"value": "Disabled", "label": "Disabled"}
    		]
    },  
    {
    	"label": "Reset",
    	"name": "reset",
    	"title": "Reset all settings back:",
	"type": "control"
    }]
}
